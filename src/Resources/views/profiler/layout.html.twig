{% extends '@WebProfiler/Profiler/layout.html.twig' %}

{% block toolbar %}
    {% if collector.groupedResults|length > 0 %}
        {% set icon %}
            <div class="sf-toolbar-icon">
                <img src="data:image/png;base64," alt="Feature toggle"/>
                <span class="sf-toolbar-value">{{ collector.groupedResults|length }} features</span>
            </div>
        {% endset %}

        {{ include('@WebProfiler/Profiler/toolbar_item.html.twig', { link: true }) }}
    {% endif %}
{% endblock %}

{% block menu %}
    <span class="label {%- if collector.groupedResults|length == 0 %} disabled{% endif %}">
        <span class="icon"><img src="data:image/png;base64," alt="Feature toggle"/></span>
        <strong>Features</strong>
        {% if collector.groupedResults|length > 0 %}
            <span class="count">
                <span>{{ collector.groupedResults|length }}</span>
            </span>
        {% endif %}
    </span>
{% endblock %}

{% block panel %}
    <h2>Dashboard</h2>
    <table>
        <thead>
        <tr>
            <th class="col">Feature name</th>
            <th class="col">Amount requests</th>
            <th class="col">Amount active</th>
            <th class="col">Amount inactive</th>
            <th class="col">Activator</th>
        </tr>
        </thead>
        {% for group in collector.groupedResults %}
            <tr>
                <td>{{ group.featureName }}</td>
                <td>{{ group.amountRequests }}</td>
                <td>{{ group.amountActive }}</td>
                <td>{{ group.amountInactive }}</td>
                <td>{{ group.activators|join(', ') }}</td>
            </tr>
        {% endfor %}
    </table>

    <h2>Log</h2>
    <table>
        <thead>
        <tr>
            <th class="col">Feature name</th>
            <th class="col">Active</th>
            <th class="col">Activator</th>
            <th class="col">Context</th>
        </tr>
        </thead>
        {% for result in collector.results %}
            <tr>
                <td>{{ result.featureName }}</td>
                <td>{{ result.active ? 'true' : 'false' }}</td>
                <td>{{ result.activator }}</td>
                <td>{{ dump(result.context.all) }}</td>
            </tr>
        {% endfor %}
    </table>
{% endblock %}
